<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<GridPane xmlns:fx="http://javafx.com/fxml" fx:controller="org.spica.fx.controllers.ProjectsController"
          alignment="TOP_LEFT" hgap="10" vgap="10">
    <stylesheets>
        <URL value="@/css/spica.css"/>
    </stylesheets>
    <padding>
        <Insets top="20" bottom="20" left="20" right="20"/>
    </padding>

    <HBox GridPane.rowIndex="0" GridPane.columnIndex="0" GridPane.columnSpan="2" spacing="20">
        <TextField fx:id="txtSearch" minWidth="400"></TextField>
    </HBox >
    <TreeView fx:id="treProjects" GridPane.rowIndex="1" GridPane.columnIndex="0" GridPane.fillWidth="true" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS"></TreeView>


</GridPane>